doctype html
html
	head
		title List Page
	body
		h1 List Page
		a(href='/insert') INSERT DATA
		hr
		table(width='100%' border='1')
			tr
				th DELETE
				th EDIT
				th ID
				th Name
				th Model Number
				th Series
			<!-- 반복문 : each 변수 in 받아온 데이타 -->
			each item in data
				tr
					td
						a(href='/delete/'+item.id) DELETE
					td
						a(href='/edit/'+item.id) EDIT
					td
						=item.id
					td
						=item.name
					td
						=item.modelnumber
					td
						=item.series
			

